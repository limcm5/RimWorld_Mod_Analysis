<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!--============ Royalty DLC - StockGenerators =================-->
  	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
		<match Class="PatchOperationSequence">
		<operations>

<!--============ Combined Research Samples - StockGenerator =================-->
	<li Class="PatchOperationAdd">
    <xpath>Defs/TraderKindDef[defName="Base_Empire_Standard" or defName="Empire_Caravan_TraderGeneral" or defName="Orbital_Empire"]/stockGenerators</xpath>
		<value>	
			<li Class="StockGenerator_SingleDef">
				<thingDef>CombinedResearchSamples</thingDef>
				<countRange>0~3</countRange>
			</li>			
		</value>
	</li>

<!--============ Theory Paper - StockGenerator =================-->
	<li Class="PatchOperationAdd">
    <xpath>Defs/TraderKindDef[defName="Base_Empire_Standard" or defName="Empire_Caravan_TraderGeneral" or defName="Orbital_Empire"]/stockGenerators</xpath>
		<value>	
			<li Class="StockGenerator_SingleDef">
				<thingDef>TheoryPapers</thingDef>
				<countRange>1~3</countRange>
			</li>			
		</value>
	</li>
	
<!--============ Theory Data - StockGenerator =================-->
	<li Class="PatchOperationAdd">
    <xpath>Defs/TraderKindDef[defName="Base_Empire_Standard" or defName="Empire_Caravan_TraderGeneral" or defName="Orbital_Empire"]/stockGenerators</xpath>
		<value>		
			<li Class="StockGenerator_SingleDef">
				<thingDef>TheoryData</thingDef>
				<countRange>0~2</countRange>
			</li>			
		</value>
	</li>
		
 		</operations>
		</match>
	</Operation> 
  
</Patch>