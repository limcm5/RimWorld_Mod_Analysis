		</RoyalTitleDef>

		<RoyalTitlePermitDef>
			<defName>MR_CallHunter</defName>
			<label>Call Hired Hunter</label>
			<description>Call a group of 8 Maru Hunters to aid you in battle.</description>
			<workerClass>RoyalTitlePermitWorker_CallAid</workerClass>
			<minTitle>MRFT_Maru</minTitle>
			<faction>MRF_Hyperborea</faction>
			<permitPointCost>1</permitPointCost>
			<uiPosition>(0,0)</uiPosition>
			<royalAid>
				<favorCost>6</favorCost>
				<pawnKindDef>MRP_Hunter</pawnKindDef>
				<pawnCount>8</pawnCount>
				<targetingRange>39.9</targetingRange>
			</royalAid>
			<cooldownDays>36</cooldownDays>
		</RoyalTitlePermitDef>

		<RoyalTitlePermitDef>
			<defName>MR_Callfang</defName>
			<label>Call Fangs</label>
			<description>Call a group of 8 Maru fangs to aid you in battle.</description>
			<workerClass>RoyalTitlePermitWorker_CallAid</workerClass>
			<minTitle>MRFT_Decipher</minTitle>
			<prerequisite>MR_CallHunter</prerequisite>
			<faction>MRF_Hyperborea</faction>
			<permitPointCost>1</permitPointCost>
			<uiPosition>(1,0)</uiPosition>
			<royalAid>
				<favorCost>6</favorCost>
				<pawnKindDef>MRP_fangs_A</pawnKindDef>
				<pawnCount>10</pawnCount>
				<targetingRange>39.9</targetingRange>
			</royalAid>
			<cooldownDays>60</cooldownDays>
		</RoyalTitlePermitDef>
		
		
		
<RoyalTitlePermitDef Name="MRF_DropResourcesBase" Abstract="True">
