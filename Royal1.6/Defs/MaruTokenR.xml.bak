    <RecipeDef ParentName="MR_TokenletherBase">
    <jobString>Dyeing leather.</jobString>
    <workAmount>600</workAmount>
    <defName>MRMake_lether</defName>
    <label>Leather dyeing</label>
    <description>The leather is dyed with a unique pattern on the Maru.</description>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Leathers</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Leathers</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <MR_dyedleather>1</MR_dyedleather>
    </products>
  </RecipeDef>
  
      <RecipeDef ParentName="MR_TokenrecordBase">
    <jobString>Writing the Record.</jobString>
    <workAmount>3000</workAmount>
    <defName>MRMake_record</defName>
    <label>Record writing</label>
    <description>Interpret sariras and write analysis notes.</description>
    <ingredients>
	  <li>
        <filter>
          <thingDefs>
            <li>MR_Sarira</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MR_Sarira</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <MR_Record>1</MR_Record>
    </products>
    <researchPrerequisite>MaruRD1</researchPrerequisite>
    <skillRequirements>
      <Intellectual>8</Intellectual>
    </skillRequirements>
  </RecipeDef>
  
