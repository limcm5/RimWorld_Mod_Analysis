  <!--띵크트리-->
  
 <ThinkTreeDef>
    <defName>MR_AnimalConstant_snowleopard</defName>
    <thinkRoot Class="ThinkNode_Priority">
      <subNodes>
        <!-- Despawned -->
        <li Class="ThinkNode_Subtree">
          <treeDef>Despawned</treeDef>
        </li>
			
        <li Class="ThinkNode_ConditionalCanDoConstantThinkTreeJobNow">
          <subNodes>
            <!-- Flee enemies, tornados, meteorites, etc. -->
            <li Class="JobGiver_AnimalFlee" />
            <!-- Join auto joinable caravan -->
            <li Class="ThinkNode_Subtree">
              <treeDef>JoinAutoJoinableCaravan</treeDef>
            </li>
          </subNodes>
        </li>

        <li Class="ThinkNode_ConditionalCanDoLordJobNow">
          <subNodes>
            <!-- Lord directives -->
            <li Class="ThinkNode_Subtree">
              <treeDef>LordDutyConstant</treeDef>
            </li>
          </subNodes>
        </li>
		
			
      </subNodes>
    </thinkRoot>
  </ThinkTreeDef>
  
  
  <HediffDef>
		<defName>MRH_RageHediff</defName>
		<hediffClass>HediffWithComps</hediffClass>
		<label>particleization</label>
		<!--입자화-->
		<initialSeverity>0.001</initialSeverity>
		<labelNoun>particleization</labelNoun>
		<!--입자화-->
		<description>In response to external stimuli, the body becomes particles and momentarily escapes the limitations of the physical body.</description>
		<!--외부 자극에 반응해 몸을 입자화시켜 순간적으로 육체의 한계를 벗어납니다-->
		<defaultLabelColor>(0.7,0.3,0.3)</defaultLabelColor>
		<scenarioCanAdd>false</scenarioCanAdd>
		<maxSeverity>1.0</maxSeverity>
		<isBad>false</isBad>
		<comps>
      <li Class="HediffCompProperties_Effecter">
        <stateEffecter>MR_MoteMaruRage</stateEffecter>
        <severityIndices>0~5</severityIndices>
      </li>
		</comps>
		<stages>
			<li>
				<statFactors>
                <MoveSpeed>3</MoveSpeed>
                <MeleeCooldownFactor>2</MeleeCooldownFactor>
				<IncomingDamageFactor>0</IncomingDamageFactor>
				</statFactors>
			</li>
		</stages>
	</HediffDef>
	
