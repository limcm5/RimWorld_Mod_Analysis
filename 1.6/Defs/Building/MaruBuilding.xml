    <costList>
      <MR_thurible_item>1</MR_thurible_item>
    </costList>
    <building>
      <buildingTags>
        <li>Maruthurible</li>
      </buildingTags>
      <destroySound>BuildingDestroyed_Metal_Medium</destroySound>
